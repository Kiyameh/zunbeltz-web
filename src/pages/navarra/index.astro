---
import HtmlLayout from "@/layouts/HtmlLayout.astro";
import type { MetaInfo } from "@/components/SEO/MetaInfo";

const meta: MetaInfo = {
  pageTitle: "Espeleología y Aventura en Navarra | Guía Zunbeltz",
  pageDescription:
    "Explora el subsuelo y las cumbres de la Comunidad Foral. Guía de cuevas, barrancos y montes de Navarra.",
  pageTags: [
    "Navarra",
    "Turismo activo",
    "Cuevas Navarra",
    "Pirineo Navarro",
    "Aventura",
  ],
  pageUrl: `https://zunbeltz.org/navarra`,
};
---

<HtmlLayout metaInfo={meta}>
  <main>
    <h1>Navarra</h1>
  </main>
</HtmlLayout>

<style>
  main {
    min-height: 100vh;
    max-width: 56rem;
    margin-left: auto;
    margin-right: auto;
    padding: 3rem 1.5rem;
  }
</style>
