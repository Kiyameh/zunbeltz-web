---
import HtmlLayout from "@/layouts/HtmlLayout.astro";
import HeaderLayout from "@/layouts/HeaderLayout.astro";
import FooterLayout from "@/layouts/FooterLayout.astro";
---

<HtmlLayout
  pageTitle="Navarra | Zunbeltz"
  pageDescription="Descubre la naturaleza de Navarra: cuevas, montañas, ríos y paisajes únicos"
  pageTags={[
    "navarra",
    "naturaleza",
    "cuevas",
    "montañas",
    "ríos",
    "espeleología",
    "paisajes",
  ]}
>
  <HeaderLayout />
  <main class="min-h-screen bg-white max-w-4xl mx-auto px-6 py-12">
    <h1>Navarra</h1>
  </main>
  <FooterLayout />
</HtmlLayout>
