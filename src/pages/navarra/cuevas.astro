---
import HtmlLayout from "@/layouts/HtmlLayout.astro";
import type { MetaInfo } from "@/components/SEO/MetaInfo";
import og_cuevas from "@/assets/opengraph/og_cuevas.jpg";

const meta: MetaInfo = {
  pageTitle: "Cuevas y Simas de Navarra | Zunbeltz",
  pageDescription:
    "Zonas kársticas de Navarra y sus cavidades. Explora el mundo subterráneo de la provincia",
  pageTags: ["Cuevas", "Navarra", "Simas", "Larra", "Urbasa", "Aralar"],
  pageImage: og_cuevas.src,
  pageUrl: `https://zunbeltz.org/navarra/cuevas`,
};
---

<HtmlLayout metaInfo={meta}>
  <main>
    <h1>Cuevas de Navarra</h1>
  </main>
</HtmlLayout>

<style>
  main {
    min-height: 100vh;
    max-width: 56rem;
    margin-left: auto;
    margin-right: auto;
    padding: 3rem 1.5rem;
  }
</style>
