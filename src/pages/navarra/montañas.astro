---
import HtmlLayout from "@/layouts/HtmlLayout.astro";
import HeaderLayout from "@/layouts/HeaderLayout.astro";
import FooterLayout from "@/layouts/FooterLayout.astro";
---

<HtmlLayout
  pageTitle="Montañas de Navarra | Zunbeltz"
  pageDescription="Las montañas y paisajes de la provincia de Navarra. Descubre los picos y sierras más emblemáticos"
  pageTags={[
    "montañas",
    "navarra",
    "senderismo",
    "paisajes",
    "naturaleza",
    "picos",
  ]}
>
  <HeaderLayout />
  <main class="min-h-screen bg-white max-w-4xl mx-auto px-6 py-12">
    <h1>Montañas de Navarra</h1>
  </main>
  <FooterLayout />
</HtmlLayout>
